{
  "extends": [
    "next",
    "next/core-web-vitals",
    "eslint:recommended"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "import/order": [
      2,
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "no-restricted-imports": ["error", { "patterns": ["./", "../"] }]
  }
}
